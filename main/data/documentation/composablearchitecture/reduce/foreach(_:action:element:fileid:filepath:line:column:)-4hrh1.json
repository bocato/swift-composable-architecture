ifier","text":"Element"},{"text":".","kind":"text"},{"kind":"typeIdentifier","text":"Action","preciseIdentifier":"s:22ComposableArchitecture7ReducerP6ActionQa","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action"},{"text":", ","kind":"text"},{"text":"ID","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:SH","text":"Hashable","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"ID","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:s8SendableP","text":"Sendable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Element"},{"text":" : ","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","text":"Reducer"},{"kind":"text","text":"\n"}]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"toElementsState","content":[{"inlineContent":[{"type":"text","text":"A writable key path from parent state to an "},{"code":"IdentifiedArray","type":"codeVoice"},{"type":"text","text":" of child"},{"text":" ","type":"text"},{"type":"text","text":"state."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent action to an ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction"},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"actions."}]}],"name":"toElementAction"},{"name":"element","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against elements of child"},{"text":" ","type":"text"},{"text":"state.","type":"text"}]}]},{"name":"fileID","content":[{"inlineContent":[{"type":"text","text":"The fileID."}],"type":"paragraph"}]},{"name":"filePath","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The filePath."}]}]},{"name":"line","content":[{"inlineContent":[{"type":"text","text":"The line."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"The column."}],"type":"paragraph"}],"name":"column"}]},{"content":[{"anchor":"return-value","text":"Return Value","level":2,"type":"heading"},{"inlineContent":[{"text":"A reducer that combines the child reducer with the parent reducer.","type":"text"}],"type":"paragraph"}],"kind":"content"},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto an array of child states, then it can perform"},{"text":" ","type":"text"},{"type":"text","text":"its core logic "},{"type":"emphasis","inlineContent":[{"text":"and","type":"text"}]},{"type":"text","text":" the child’s logic by using the "},{"code":"forEach","type":"codeVoice"},{"text":" operator:","type":"text"}],"type":"paragraph"},{"type":"codeListing","code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"],"syntax":"swift"},{"style":"tip","content":[{"inlineContent":[{"text":"We are using ","type":"text"},{"type":"codeVoice","code":"IdentifiedArray"},{"text":" from our","type":"text"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections"},{"text":" library because it provides a safe","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and ergonomic API for accessing elements from a stable ID rather tha